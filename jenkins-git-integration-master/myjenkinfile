pipeline {
  agent { label 'label1' }
  stages {
    stage ('BUILD') {
      steps {
        echo "cloning java project from git"
        sh ''' 
		    git clone <java_proj_git_url>
	   '''
        }
    }
   stage ('Clone') {
		agent { label 'lable1' }
		steps {
			echo "Build a binary"
        sh ''' 
			mvn clean package
	   '''
      }
    }
  }
}
